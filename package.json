{
  "name": "nyt-scaper",
  "version": "1.2.0",
  "description": "New York Times web scraper.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn client\"",
    "client": "cd client && yarn start",
    "install": "cd client && yarn install",
    "build": "cd client && yarn build",
    "heroku-postbuild": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrsantacruz86/NYTimesScraper.git"
  },
  "keywords": [
    "express",
    "handlebars",
    "mysql",
    "scraper"
  ],
  "author": "Nelson Diaz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrsantacruz86/NYTimesScraper/issues"
  },
  "homepage": "https://github.com/mrsantacruz86/NYTimesScraper#readme",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.3",
    "he": "^1.1.1",
    "mongoose": "^5.1.3",
    "if-env": "^1.0.4",
    "morgan": "^1.9.0"
  }
}
